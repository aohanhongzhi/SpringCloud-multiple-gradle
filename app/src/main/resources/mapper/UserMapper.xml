<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hxy.dream.dao.mapper.UserMapper">
    <update id="updateWithoutLogicDelete">
        update user_model
        set name    = #{name},
            age     = #{age},
            deleted = 0
        where id = #{id}
    </update>
</mapper>
